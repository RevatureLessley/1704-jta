<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>HelloWorldServlet</servlet-name>
  	<display-name>HelloWorldServlet</display-name>
  	<description></description>
  	<servlet-class>com.revature.servlets.HelloWorldServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>MasterServlet</servlet-name>
  	<display-name>MasterServlet</display-name>
  	<description></description>
  	<servlet-class>com.revature.servlets.MasterServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ReimbursementServlet</servlet-name>
  	<display-name>ReimbursementServlet</display-name>
  	<description></description>
  	<servlet-class>com.revature.servlets.ReimbursementServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AjaxServlet</servlet-name>
  	<display-name>AjaxServlet</display-name>
  	<description></description>
  	<servlet-class>com.revature.servlets.AjaxServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>HelloWorldServlet</servlet-name>
  	<url-pattern>/HelloWorldServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>MasterServlet</servlet-name>
  	<url-pattern>*.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ReimbursementServlet</servlet-name>
  	<url-pattern>/ReimbursementServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AjaxServlet</servlet-name>
  	<url-pattern>*.ajax</url-pattern>
  </servlet-mapping>
</web-app>
